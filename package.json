{
  "name": "tylerschmidt_v3",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "preview": "next build && next start",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "node scripts/verify-build.js",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "verify": "node scripts/verify-build.js",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "sitemap": "next-sitemap",
    "dev:check": "npm run type-check && npm run lint && npm run dev",
    "check:entities": "node scripts/check-unescaped-entities.js",
    "precommit": "npm run format && npm run lint && npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@portabletext/react": "^3.2.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-three/drei": "^10.6.0",
    "@react-three/fiber": "^9.2.0",
    "@react-three/postprocessing": "^3.0.4",
    "@sanity/client": "^7.8.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^4.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.7",
    "gsap": "^3.13.0",
    "lucide-react": "^0.540.0",
    "next": "15.4.3",
    "next-sanity": "^10.0.12",
    "plaiceholder": "^3.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanity": "^4.4.1",
    "sharp": "^0.34.3",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.178.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
